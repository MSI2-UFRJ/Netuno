<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<converter>
		<converter-id>portoConverter</converter-id>
		<converter-class>br.com.ufrj.msi2.netuno.converter.PortoConverter</converter-class>
	</converter>
	<navigation-rule>
		<description>Tela de login do Netuno</description>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{Login.autenticar}</from-action>
			<from-outcome>logado</from-outcome>
			<to-view-id>/dashboard.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{Login.autenticar}</from-action>
			<from-outcome>agenteCarga</from-outcome>
			<to-view-id>/cargas/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{Login.autenticar}</from-action>
			<from-outcome>contratante</from-outcome>
			<to-view-id>/contratacao/verContratos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{Login.autenticar}</from-action>
			<from-outcome>agentelogistica</from-outcome>
			<to-view-id>/logistica/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{Login.autenticar}</from-action>
			<from-outcome>agentePorto</from-outcome>
			<to-view-id>/portos/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{Login.autenticar}</from-action>
			<from-outcome>atendente</from-outcome>
			<to-view-id>/contratacao/atendente/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/logistica/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>verPregoes</from-outcome>
			<to-view-id>/logistica/pregoes/listar.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadEmpTrans</from-outcome>
			<to-view-id>/logistica/transporte/cadEmpTrans.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>abrirPregao</from-outcome>
			<to-view-id>/logistica/pregoes/abrir.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/portos/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>verPortos</from-outcome>
			<to-view-id>/portos/listar.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/contratacao/verContratos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>contratacao</from-outcome>
			<to-view-id>/contratacao/contratacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/contratacao/atendente/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>contratacao</from-outcome>
			<to-view-id>/contratacao/contratacao.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>novoContratante</from-outcome>
			<to-view-id>/contratacao/atendente/novoContratante.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/contratacao/atendente/novoContratante.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atendente</from-outcome>
			<to-view-id>/contratacao/atendente/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>contratacao</from-outcome>
			<to-view-id>/contratacao/contratacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/contratacao/contratacao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fecharContrato</from-outcome>
			<to-view-id>/contratacao/fecharContrato.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>verContratos</from-outcome>
			<to-view-id>/contratacao/verContratos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>telaAtendente</from-outcome>
			<to-view-id>/contratacao/atendente/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/contratacao/fecharContrato.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>contratacao</from-outcome>
			<to-view-id>/contratacao/contratacao.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>verContratos</from-outcome>
			<to-view-id>/contratacao/verContratos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>telaAtendente</from-outcome>
			<to-view-id>/contratacao/atendente/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/logistica/transporte/cadEmpTrans.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>voltarLogistica</from-outcome>
			<to-view-id>/logistica/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/cargas/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaNaviosAgente</from-outcome>
			<to-view-id>/cargas/listaNaviosAgente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/cargas/listaNaviosAgente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaConteinersNavio</from-outcome>
			<to-view-id>/cargas/listaConteinersNavioAgente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Definicao do arquivo de mensagens -->
	<application>
		<resource-bundle>
			<base-name>br.com.ufrj.msi2.netuno.resources.ContratacaoMessages</base-name>
			<var>contratacaoMsg</var>
		</resource-bundle>
		<locale-config>
			<default-locale>pt</default-locale>
			<supported-locale>pt</supported-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
</faces-config>